<%=flash[:notice]%>
<table>
	<thread>
	<tr>
		<th>Id</th>
		<th>Name</th>
		<th>Marks</th>
		<th>Percentage</th>
		
	</tr>
	</thread>
	<tbody>
		<% @placements.each do |placement| %>
			<tr>
				<td><%=placement.id%></td>
				<td><%=placement.Name%></td>
				<td><%=placement.Marks%></td>
				<td><%=placement.Percentage%></td>
			</tr>
		<%end%>
	</tbody>
</table>
<div>
<h3>Import a CSV File</h3>
	<%= form_tag import_placements_path, multipart: true do %>
		<%= file_field_tag :file %>
		<%= submit_tag "Import CSV" %>
	<%end%>
</div>