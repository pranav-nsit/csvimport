<%=flash[:notice]%>
<table>
	<thread>
	<tr>
		<th>Id</th>
		<th>Name</th>
		<th>Roll</th>
		<th>Marks</th>
		
	</tr>
	</thread>
	<tbody>
		<% @coc.each do |codeofconduct| %>
			<tr>
				<td><%=coc.id%></td>
				<td><%=coc.Name%></td>
				<td><%=coc.Roll%></td>
				<td><%=coc.Marks%></td>
			</tr>
		<%end%>
	</tbody>
</table>
<div>
<h3>Import a CSV File</h3>
	<%= form_tag import_codeofconduct_path, multipart: true do %>
		<%= file_field_tag :file %>
		<%= submit_tag "Import CSV" %>
	<%end%>
</div>